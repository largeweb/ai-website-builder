if [ -z "$1" ]
then
	echo "PROVIDE API KEY -- ./openai-request API_KEY"
else
	curl https://api.openai.com/v1/completions -H "Content-Type: application/json" -H "Authorization: Bearer $1" -d '{"model": "text-davinci-002", "prompt": "write some program in html", "temperature": 0, "max_tokens": 100}'
fi
